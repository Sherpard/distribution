[org.seedstack.seed]
core.application-id = ${rootArtifactId}
# The following property exposes REST resources on / instead of the /rest default
rest.path =

# The following section is used by ConfigurationRealm, the default security realm, which should be replaced by a
# more useful one like LdapRealm.
# Check https://www.seedstack.org/docs/seed/manual/security/.

[org.seedstack.seed.security.users]
username = password

# Uncomment to add basic auth
#[org.seedstack.seed.security.urls]
#/** = authcBasic

# The following section resets to properties without prefix
[]
